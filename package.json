{
  "name": "aesp-root",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:all": "concurrently -n BE,FE -c green,blue \"npm run dev:be\" \"npm run dev:fe\"",
    "dev:be": "nodemon backend/src/server.js",
    "dev:fe": "vite --config frontend/vite.config.js --open",
    "seed:admin": "node backend/seed/seedAdminsFromFile.js",
    "install:all": "npm install",
    "build": "npm run build:fe",
    "build:fe": "vite build --config frontend/vite.config.js",
    "start": "node backend/src/server.js"
  },
  "dependencies": {
    "axios": "^1.13.1",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^18.2.0",
    "react-easy-crop": "^5.5.3",
    "react-icons": "^5.5.0",
    "react-modal": "^3.16.3",
    "react-router-dom": "^7.9.5",
    "recharts": "^3.3.0",
    "swiper": "^12.0.3"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^8.2.2",
    "cross-port-killer": "^1.4.0",
    "nodemon": "^3.1.10",
    "vite": "^5.4.21"
  }
}
